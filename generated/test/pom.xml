	<properties>
		<java.version>1.8</java.version>
		<slim.version>0.2.3-SNAPSHOT</slim.version>
	</properties>

	<build>
		<plugins>
			<plugin>
				<artifactId>maven-clean-plugin</artifactId>
				<configuration>
					<filesets>
						<fileset>
							<directory>src/main/resources</directory>
							<includes>
								<include>META-INF/**/*</include>
							</includes>
						</fileset>
					</filesets>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.springframework.experimental</groupId>
				<artifactId>spring-init-maven-plugin</artifactId>
				<version>${slim.version}</version>
				<executions>
					<execution>
						<id>sources</id>
						<phase>process-sources</phase>
						<goals>
							<goal>generate</goal>
						</goals>
						<inherited>false</inherited>
						<configuration>
							<basePackage>org.springframework.boot.test</basePackage>
							<force>true</force>
						</configuration>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>

</project>
