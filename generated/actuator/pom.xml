	<properties>
		<java.version>1.8</java.version>
		<slim.version>0.3.1-SNAPSHOT</slim.version>
	</properties>

	<build>
		<plugins>
			<plugin>
				<groupId>org.springframework.experimental</groupId>
				<artifactId>spring-init-maven-plugin</artifactId>
				<version>${slim.version}</version>
				<executions>
					<execution>
						<id>sources</id>
						<phase>process-sources</phase>
						<goals>
							<goal>generate</goal>
						</goals>
						<inherited>false</inherited>
						<configuration>
							<basePackage>org.springframework.boot.actuate.autoconfigure</basePackage>
							<force>true</force>
							<closedWorld>true</closedWorld>
							<nativeImageDirectory>${project.build.directory}/generated-resources/init</nativeImageDirectory>
						</configuration>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>

